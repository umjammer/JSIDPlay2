<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE xml>
<assembly xmlns="http://maven.apache.org/ASSEMBLY/2.0.0"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://maven.apache.org/ASSEMBLY/2.0.0 http://maven.apache.org/xsd/assembly-2.0.0.xsd">
	<id>wasm-zip</id>
	<formats>
		<format>zip</format>
	</formats>
	<fileSets>
		<fileSet>
			<directory>${project.build.directory}/classes/server/restful/webapp/teavm/wasm/</directory>
			<outputDirectory>wasm</outputDirectory>
			<filtered>false</filtered>
		</fileSet>
		<fileSet>
			<directory>${project.build.directory}/classes/server/restful/webapp/teavm/</directory>
			<outputDirectory></outputDirectory>
			<filtered>true</filtered>
			<includes>
				<include>c64jukebox.vue</include>
				<include>c64jukebox.scss</include>
				<include>test-*.html</include>
				<include>README</include>
				<include>test.html</include>
			</includes>
			<excludes>
				<exclude>wasm</exclude>
				<exclude>js</exclude>
			</excludes>
		</fileSet>
	</fileSets>
</assembly>